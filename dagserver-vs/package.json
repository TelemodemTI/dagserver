{
  "name": "dagserver-vs",
  "displayName": "dagserver-vs",
  "description": "dagserver pluging",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dagserver-vs.helloWorld",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dagserver-vs.loadView",
        "title": "Load Job Detail"
      }
    ],
    "views": {
			"explorer": [
				{
					"id": "explorer",
					"name": "DagsExplorer"
				}
			]
		},
    "configuration": {
      "id": "DagserverCredentials",
      "title": "Dagserver Credentials",
      "properties": {
        "host": {
          "type": "string",
          "description": "Dagserver host",
          "default": "ws://localhost:8080/dagserver"
        },
        "username": {
          "type": "string",
          "description": "Dagserver Username",
          "default": "dagserver"
        },
        "password": {
          "type": "string",
          "description": "Dagserver Password",
          "default": "dagserver"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.73.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "ws": "^8.5.0"
  }
}
